{
	"name": "csg-builder",
	"version": "1.4.0",
	"type": "module",
	"main": "build/index.js",
	"engines": {
		"node": ">=22.0.0",
		"npm": ">=10.0.0"
	},
	"scripts": {
		"dev": "vite dev --host",
		"build": "vite build",
		"preview": "vite preview",
		"ts:check": "svelte-check --tsconfig ./tsconfig.json",
		"format:check": "prettier --check .",
		"format:fix": "prettier --write .",
		"lint:check": "eslint .",
		"lint:fix": "eslint --fix .",
		"npm:reinstall": "rm -rf ./node_modules && rm -f ./package-lock.json && npm i && npm i",
		"all": "npm run format:fix && npm run lint:fix && npm run ts:check && npm run build"
	},
	"devDependencies": {
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/postcss": "^4.1.14",
		"@tailwindcss/vite": "^4.1.14",
		"@tsconfig/svelte": "^5.0.5",
		"@types/three": "^0.180.0",
		"@typescript-eslint/eslint-plugin": "^8.45.0",
		"@typescript-eslint/parser": "^8.45.0",
		"autoprefixer": "^10.4.21",
		"eslint": "^9.37.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-simple-import-sort": "^12.1.1",
		"eslint-plugin-svelte": "^3.12.4",
		"eslint-plugin-unicorn": "^61.0.2",
		"flowbite": "^3.1.2",
		"flowbite-svelte": "^1.17.1",
		"postcss": "^8.5.6",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"svelte": "^5.39.8",
		"svelte-check": "^4.3.2",
		"svelte-preprocess": "^6.0.3",
		"tailwindcss": "^4.1.14",
		"tslib": "^2.8.1",
		"typescript": "^5.9.3",
		"vite": "^7.1.9"
	},
	"dependencies": {
		"@threlte/core": "^8.1.5",
		"@threlte/extras": "^9.5.4",
		"ieee754": "^1.2.1",
		"three": "^0.180.0",
		"three-bvh-csg": "^0.0.17"
	}
}
